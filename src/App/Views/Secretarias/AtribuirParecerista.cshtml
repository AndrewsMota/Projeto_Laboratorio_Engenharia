@model App.ViewModels.AtribuirPareceristaViewModel
@{
    ViewData["Title"] = "Atribuir Parecerista";
}

<h1>@ViewData["Title"]</h1>

<div class="row">
    <div class="col-md-4">
        <form enctype="multipart/form-data" asp-action="Create">
            <hr />
            <div class="form-group">
                <label asp-for="ProtocoloId" class="control-label"></label>
                <input asp-for="ProtocoloId" readonly class="form-control" />
                <span asp-validation-for="ProtocoloId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PareceristaId" class="control-label"></label>
                <select asp-for="PareceristaId" class="form-control" asp-items="@(new SelectList(Model.Pareceristas, "Id", "UserInfo.NomeCompleto"))"></select>
                <span asp-validation-for="PareceristaId" class="text-danger"></span>
            </div>
            <button class="btn btn-primary" asp-action="AtribuirParecerista">Atribuir</button>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

