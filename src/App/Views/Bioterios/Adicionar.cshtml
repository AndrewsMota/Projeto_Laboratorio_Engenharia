@model App.ViewModels.BioterioViewModel
@{
    ViewData["Title"] = "Cadastro Biotério";
}

<h1>@ViewData["Title"]</h1>

<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <h4>Cadastrar novo biotério.</h4>
            <hr />
            <partial name="_NovoBioterioForm" />
            <button class="btn btn-primary" asp-action="Adicionar">Cadastrar</button>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}

    <script>
        BuscaCep();
    </script>
}

